<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.braintreepayments.demo.samsungpay"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".MainKotlinActivity"/>
        <activity android:name=".MainActivity"/>
        <meta-data android:name="debug_mode" android:value="Y" />
        <meta-data android:name="spay_debug_api_key" android:value="588f2ecd9d9d4b94805494714f3f7e27" />
    </application>

</manifest>
